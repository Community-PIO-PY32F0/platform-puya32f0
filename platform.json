{
  "name": "puya32f0",
  "title": "PUYA PY32F0",
  "description": "PUYA PY32F0 Arm Cortex-M0+ MCU development platform",
  "homepage": "https://www.puyasemi.com/mcu_weichuliqi.html",
  "license": "Apache-2.0",
  "keywords": ["PUYA", "PY32F0", "ARM", "dev-platform", "Cortex-M"],
  "repository": {
    "type": "git",
    "url": "https://github.com/Community-PIO-PY32F0/platform-puya32f0.git"
  },
  "version": "0.0.1",
  "engines": {
    "platformio": "^6"
  },
  "frameworks": {
    "puya-py32f0-ll-sdk": {
      "package": "framework-puya-py32f0-ll-sdk",
      "script": "builder/frameworks/ll_sdk.py",
      "description": "PUYA PY32F0 Low-Level SDK",
      "title": "PUYA PY32F0 Low-Level SDK",
      "homepage": "https://www.puyasemi.com/mcu_weichuliqi.html"
    },
    "puya-py32f0-hal-sdk": {
      "package": "framework-puya-py32f0-hal-sdk",
      "script": "builder/frameworks/hal_sdk.py",
      "description": "PUYA PY32F0 HAL SDK",
      "title": "PUYA PY32F0 HAL SDK",
      "homepage": "https://www.puyasemi.com/mcu_weichuliqi.html"
    }
  },
  "packages": {
    "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "optional": false,
      "owner": "platformio",
      "version": ">=1.120301.0"
    },
    "tool-pyocd": {
      "type": "uploader",
      "optional": false,
      "owner": "platformio",
      "version": ">=2.3600.0"
    },
    "framework-puya-py32f0-ll-sdk": {
      "type": "framework",
      "optional": false,
      "owner": "platformio",
      "version": "https://github.com/Community-PIO-PY32F0/framework-puya-py32f0-ll-sdk.git"
    },
    "framework-puya-py32f0-cmsis": {
      "type": "framework",
      "optional": false,
      "owner": "platformio",
      "version": "https://github.com/Community-PIO-PY32F0/framework-puya-py32f0-cmsis.git"
    },
    "framework-puya-py32f0-hal-sdk": {
      "type": "framework",
      "optional": false,
      "owner": "platformio",
      "version": "https://github.com/Community-PIO-PY32F0/framework-puya-py32f0-hal-sdk.git"
    }
  }
}
